<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PCInfoController</servlet-name>
        <servlet-class>Controler.PCInfoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PCDatabaseCon</servlet-name>
        <servlet-class>Controler.PCDatabaseCon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PCUpdateCon</servlet-name>
        <servlet-class>Controler.PCUpdateCon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PCIndexCon</servlet-name>
        <servlet-class>Controler.PCIndexCon</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PCInfoController</servlet-name>
        <url-pattern>/PCInfoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PCDatabaseCon</servlet-name>
        <url-pattern>/PCDatabaseCon</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file> PCIndex.jsp </welcome-file>
        <welcome-file> PCForm.jsp </welcome-file>
        <welcome-file> PCFormDetail.jsp </welcome-file>
        <welcome-file> PCDatabaseDetail.jsp </welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>PCUpdateCon</servlet-name>
        <url-pattern>/PCUpdateCon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PCIndexCon</servlet-name>
        <url-pattern>/PCIndexCon</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
